                            android:layout_marginStart="8dp"
                            android:layout_marginTop="8dp"
                            android:layout_marginEnd="8dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/jgf"
                            app:layout_constraintTop_toBottomOf="@+id/jhl"
                            tools:ignore="DuplicateIds">

                            <android.support.design.widget.TextInputEditText
                                android:id="@+id/t4"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:hint="Nombre"
                                android:inputType="number"
                                android:textSize="15sp" />
                        </android.support.design.widget.TextInputLayout>

                        <android.support.design.widget.TextInputLayout
                            android:id="@+id/ojvc"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="8dp"
                            android:layout_marginTop="8dp"
                            android:layout_marginEnd="8dp"
                            android:layout_marginBottom="8dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/asd"
                            app:layout_constraintTop_toBottomOf="@+id/khpo"
                            tools:ignore="DuplicateIds">

                            <android.support.design.widget.TextInputEditText
                                android:id="@+id/p4"
                                android:inputType="numberDecimal"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:hint="cout" />
                        </android.support.design.widget.TextInputLayout>

                        <TextView
                            android:id="@+id/jgf"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:text="Nombre Billet :"
                            app:layout_constraintBottom_toBottomOf="@+id/khpo"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="@+id/khpo"
                            tools:ignore="DuplicateIds" />

                        <TextView
                            android:id="@+id/asd"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:text="Cout Place      :"
                            app:layout_constraintBottom_toBottomOf="@+id/ojvc"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="@+id/ojvc"
                            tools:ignore="DuplicateIds" />
                    </android.support.constraint.ConstraintLayout>
                </android.support.v7.widget.CardView>

            </android.support.constraint.ConstraintLayout>

            <android.support.constraint.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="400dp"/>

        </LinearLayout>

    </ScrollView>


</android.support.constraint.ConstraintLayout>